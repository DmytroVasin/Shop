<blockquote>
  <h2>Perfumes:</h2>
</blockquote>
<% if @products.any? %>


  <%= form_tag admin_products_path, method: 'get', id: 'products_search' do %>
    <p>
      <%= text_field_tag :search, params[:search], autocomplete: :off %>
      <%= submit_tag 'Search', name: nil %>
    </p>

    <div id="perfume_table">
      <%= render 'perfume_table' %>
    </div>

  <% end %>
<% end %>
<%= link_to 'New', new_admin_product_path, class: 'btn btn-success perfume_new_btn custom_black_btn' %>