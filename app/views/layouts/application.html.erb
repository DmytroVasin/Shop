<!DOCTYPE html>
<html>
<head>
  <title>Shop</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='sitewrapper'>
    <div id="content">
      <%= render 'shared/admin' %>
      <%= render 'shared/flash_messages' %>
      <%= render 'shared/header' %>
      <%= render 'shared/main_menu' %>
      <%= yield %>
    </div>
  </div>
  <%= render 'shared/footer' %>
</body>
</html>

